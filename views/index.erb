<h1>All Articles</h1>

<% @articles.each do |article| %>
  <!-- pry! -->
  <ul>
    <li><%= article["title"] %></li>
    <li><%= article["description"] %></li>
    <li>
      <a href="<%= article["url"] %>" target="_blank">
        <%= article["url"] %>
      </a>
    </li>
  </ul>
<% end %>

<a href="/articles/new">Add a new article</a>